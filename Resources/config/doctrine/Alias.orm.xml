<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Lasso\VmailBundle\Entity\Alias" table="alias">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="created" type="datetime" column="created" nullable="false"/>
    <field name="modified" type="datetime" column="modified" nullable="false"/>
    <field name="active" type="boolean" column="active" nullable="false"/>
    <many-to-one field="destination" target-entity="Email">
      <join-columns>
        <join-column name="destination" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="source" target-entity="Email">
      <join-columns>
        <join-column name="source" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
